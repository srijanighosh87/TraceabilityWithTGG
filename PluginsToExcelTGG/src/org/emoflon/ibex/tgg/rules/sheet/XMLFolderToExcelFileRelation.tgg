#using PluginsToExcelTGG.*
#using AttrCondDefLibrary.*

#rule XMLFolderToExcelFileRelation 
#extends CreateSheetStructure 
#with PluginsToExcelTGG

#source {
	++xmlFolderDrive:Folder{
		++-subFolder->xmlFolder_1
	}
	++xmlFolder_1:Folder{
		++-subFolder->xmlFolder_2
	}
	
	++xmlFolder_2:Folder{
		++-subFolder->xmlFolder_3
	}
	
	++xmlFolder_3:Folder{
		++-subFolder->xmlFolder_workspace
	}
	
	++xmlFolder_workspace:Folder
}

#target {	
	++sheet:Sheet
	
	++excelFile:File {
		fileName:="Workspace"
		++-sheet -> sheet
	}
}

#correspondence {
	++xmlFolderToSheetRule:XMLFolderToSheetRelation{
		#src-> xmlFolder_workspace
		#trg->sheet
	}
}

#attributeConditions {

}

